    <formulas>
        <description>Used to set Created Rating</description>
        <name>CreatedRatingFormulaFIXME</name>
        <dataType>String</dataType>
        <expression>IF(
ISBLANK(TEXT({!Get_Contact.Created_Rating__c})),
&quot;Lead 0&quot; , TEXT({!Get_Contact.Created_Rating__c})
)</expression>
    </formulas>
    <formulas>
        <description>used to set cycle type</description>
        <name>CycleTypeFormulaFIXME</name>
        <dataType>String</dataType>
        <expression>IF(
ISBLANK(TEXT({!Get_Contact.Cycle_Type__c})),
&quot;New - Contact&quot; , TEXT({!Get_Contact.Cycle_Type__c})
)</expression>
    </formulas>
    <formulas>
        <description>Used to Set Funnel Group</description>
        <name>FunnelGroupFormulaFIXME</name>
        <dataType>String</dataType>
        <expression>IF(
ISBLANK(TEXT({!Get_Contact.Funnel_Group__c})),
&quot;Prospecting - AE&quot; , TEXT({!Get_Contact.Funnel_Group__c})
)</expression>
    </formulas>
    <formulas>
        <description>Used to set Lead 0 Date</description>
        <name>Lead0FormulaFIXME</name>
        <dataType>Date</dataType>
        <expression>IF(
    AND(
        OR(
            ISPICKVAL({!Get_Contact.scaleMatters__Created_Rating__c},&quot;Lead 0&quot;),
            ISPICKVAL({!Get_Contact.scaleMatters__Created_Rating__c},&quot;Lead 1&quot;),
            ISPICKVAL({!Get_Contact.scaleMatters__Created_Rating__c},&quot;Lead 2&quot;),
            ISPICKVAL({!Get_Contact.scaleMatters__Created_Rating__c},&quot;Lead 3&quot;)),
            NOT(ISBLANK({!Get_Contact.scaleMatters__Lead_0_Date__c}))),
    {!Get_Contact.scaleMatters__Lead_0_Date__c},
IF(
    AND(
        ISBLANK({!Get_Contact.scaleMatters__Lead_0_Date__c}),
        ISBLANK(TEXT({!Get_Contact.scaleMatters__Created_Rating__c}))),
    {!$Flow.CurrentDate},
IF(
    AND(
            ISPICKVAL({!Get_Contact.scaleMatters__Created_Rating__c},&quot;Lead 0&quot;),
            ISBLANK({!Get_Contact.scaleMatters__Lead_0_Date__c})),
    {!$Flow.CurrentDate},
    {!Get_Contact.scaleMatters__Lead_0_Date__c}
    )        
  )
)</expression>
    </formulas>
    <formulas>
        <description>Used to set Lead 1 Date</description>
        <name>Lead1FormulaFIXME</name>
        <dataType>Date</dataType>
        <expression>IF(
    AND(
            ISPICKVAL({!Get_Contact.Created_Rating__c},&quot;Lead 1&quot;),
            NOT(ISBLANK({!Get_Contact.Lead_1_Date__c}))),
    {!Get_Contact.Lead_1_Date__c},
    NULL
)</expression>
    </formulas>
    <formulas>
        <description>Used to set Lead 2 Bypass value</description>
        <name>Lead2BypassFormulaFIXME</name>
        <dataType>Number</dataType>
        <expression>IF(
    NOT(
        ISPICKVAL({!Get_Contact.Created_Rating__c},&quot;Lead 2&quot;)),
        1, 0
)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>Used to set Lead 2 Date</description>
        <name>Lead2FormulaFIXME</name>
        <dataType>Date</dataType>
        <expression>IF(
     NOT(ISBLANK({!Get_Contact.scaleMatters__Lead_2_Date__c})),
    {!Get_Contact.scaleMatters__Lead_2_Date__c},
{!$Flow.CurrentDate}      
)</expression>
